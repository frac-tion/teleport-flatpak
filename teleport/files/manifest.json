{
  "id" : "com.frac_tion.teleport",
  "runtime" : "org.gnome.Platform",
  "runtime-version" : "master",
  "runtime-commit" : "52f77465b8e0b3fc62096580b47954349e984bc9cd4b7ae8c0883f92a60f7a96",
  "sdk" : "org.gnome.Sdk",
  "sdk-commit" : "a928f0a62754e4cedd0b298f32d47ebebf3af289374607a774602fe02da99c81",
  "build-options" : {
    "cflags" : "-O2 -g",
    "cxxflags" : "-O2 -g",
    "env" : {
      "V" : "1"
    }
  },
  "command" : "teleport",
  "modules" : [
    {
      "name" : "avahi",
      "config-opts" : [
        "--with-distro=none",
        "--enable-gobject",
        "--with-xml=expat",
        "--disable-libdaemon",
        "--disable-core-docs",
        "--disable-manpages",
        "--disable-mono",
        "--disable-qt3",
        "--disable-qt4",
        "--disable-python",
        "--disable-gtk",
        "--disable-gtk3"
      ],
      "sources" : [
        {
          "url" : "https://avahi.org/download/avahi-0.7.tar.gz",
          "sha256" : "57a99b5dfe7fdae794e3d1ee7a62973a368e91e414bd0dfa5d84434de5b14804",
          "type" : "archive"
        }
      ],
      "cleanup" : [
        "/bin"
      ]
    },
    {
      "name" : "teleport",
      "sources" : [
        {
          "url" : "https://github.com/frac-tion/teleport.git",
          "commit" : "b93c73b2bbf7a5b4c0592710d2972cd07b001285",
          "type" : "git"
        }
      ]
    }
  ],
  "cleanup" : [
    "/include",
    "/lib/pkgconfig",
    "/share/pkgconfig",
    "/share/aclocal",
    "/man",
    "/share/man",
    "/share/gtk-doc",
    "/share/vala",
    "*.la",
    "*.a"
  ],
  "finish-args" : [
    "--share=network",
    "--share=ipc",
    "--socket=x11",
    "--socket=wayland",
    "--socket=system-bus",
    "--filesystem=home:ro",
    "--filesystem=xdg-download",
    "--filesystem=xdg-run/dconf",
    "--filesystem=~/.config/dconf:ro",
    "--talk-name=ca.desrt.dconf",
    "--env=DCONF_USER_CONFIG_DIR=.config/dconf"
  ],
  "tags" : [
    "devel"
  ],
  "desktop-file-name-suffix" : " (dev)"
}